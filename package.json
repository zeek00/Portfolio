{
  "name": "porfolio-react",
  "version": "1.0.0",
  "description": "server-side for portfolio app",
  "main": "index.js",
  "engines": {
    "node": "16.2.0",
    "npm": "8.19.2"
  },
  "scripts": {
    "start": "node index.js",
